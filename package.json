{
  "name": "goldenbot",
  "version": "1.0.0",
  "description": "Telegram Bot for the GoldenCircleChanel",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-client": "react-scripts start",
    "devfr": "cd frontend/; npm start; cd ../;",
    "dev": "npm install; nodemon index",
    "version": "heroku --version; git --version",
    "log": "heroku logs --tail",
    "buildfe": "cd frontend/; npm run build; cd ../; git add -A; git commit -a -m \"deploy frontend Build\"; git push;",
    "deploy": "npm i; npm run buildfe; heroku git:remote -a goldenbot-cvs; git push heroku master; heroku open",
    "killAll": "killall -9 node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanStaen/goldenbot.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "python"
  ],
  "dependencies": {
    "antd": "^4.16.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "lint": "^0.7.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1"
  },
  "author": "vanStaen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanStaen/goldenbot/issues"
  },
  "homepage": "https://github.com/vanStaen/goldenbot#readme"
}
